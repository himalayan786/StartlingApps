<script type="text/javascript">
var s1=1, s2=2, s3=3;
function slide()
{
if(s1<6)
{
	if(s1==1)
	{
	document.getElementById("first").href="/about";
	document.getElementById("first").title="About Us";
	document.getElementById("first1").innerHTML="About Us";
	document.getElementById("second").href="/crawler";
	document.getElementById("second").title="Startling Crawler";
	document.getElementById("third").href="/weather";
	document.getElementById("third").title="Startling Weather Forecast";
	document.getElementById("third1").innerHTML="Weather Forecast";
	document.slider1.src="/static/images/"+s1+".jpg";
	document.slider2.src="/static/images/"+s2+".jpg";
	document.slider3.src="/static/images/"+s3+".jpg";
	s1=2;s2=3;s3=4;
	}
	else if(s1==2)
	{
	document.getElementById("first").href="/crawler";
	document.getElementById("first").title="Startling Crawler";
	document.getElementById("first1").innerHTML="Startling Crawler";
	document.getElementById("second").href="/weather";
	document.getElementById("second").title="Startling Weather Forecast";
	document.getElementById("third").href="http://www.qbingapp.appspot.com/";
	document.getElementById("third").title="Qbing App";
	document.getElementById("third1").innerHTML="Qbing App";
	document.slider1.src="/static/images/"+s1+".jpg";
	document.slider2.src="/static/images/"+s2+".jpg";
	document.slider3.src="/static/images/"+s3+".jpg";
	s1=3;s2=4;s3=5;
	}
	else if(s1==3)
	{
	document.getElementById("first").href="/weather";
	document.getElementById("first").title="Startling Weather Forecast";
	document.getElementById("first1").innerHTML="Weather Forecast";
	document.getElementById("second").href="http://www.qbingapp.appspot.com/";
	document.getElementById("second").title="Qbing App";
	document.getElementById("third").href="/dictionary";
	document.getElementById("third").title="Startling Dictionary";
	document.getElementById("third1").innerHTML="Startling Dictionary";
	document.slider1.src="/static/images/"+s1+".jpg";
	document.slider2.src="/static/images/"+s2+".jpg";
	document.slider3.src="/static/images/"+s3+".jpg";
	s1=4;s2=5;s3=1;
	}
	else if(s1==4)
	{
	document.getElementById("first").href="http://www.qbingapp.appspot.com/";
	document.getElementById("first").title="Qbing App";
	document.getElementById("first1").innerHTML="Qbing App";
	document.getElementById("second").href="/dictionary";
	document.getElementById("second").title="Startling Dictionary";
	document.getElementById("third").href="/about";
	document.getElementById("third").title="About Us";
	document.getElementById("third1").innerHTML="About Us";
	document.slider1.src="/static/images/"+s1+".jpg";
	document.slider2.src="/static/images/"+s2+".jpg";
	document.slider3.src="/static/images/"+s3+".jpg";
	s1=5;s2=1;s3=2;
	}
	else if(s1==5)
	{
	document.getElementById("first").href="/dictionary";
	document.getElementById("first").title="Startling Dictionary";
	document.getElementById("first1").innerHTML="Startling Dictionary";
	document.getElementById("second").href="/about";
	document.getElementById("second").title="About Us";
	document.getElementById("third").href="/crawler";
	document.getElementById("third").title="Startling Crawler";
	document.getElementById("third1").innerHTML="Startling Crawler";
	document.slider1.src="/static/images/"+s1+".jpg";
	document.slider2.src="/static/images/"+s2+".jpg";
	document.slider3.src="/static/images/"+s3+".jpg";
	s1=1;s2=2;s3=3;
	}
}
else {s1=1;s2=2;s3=3;}
setTimeout("slide()",3500);
}
slide();
</script>
